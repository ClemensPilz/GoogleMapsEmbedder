{
  "manifest_version": 2,
  "name": "Google Maps Embedder",
  "version": "1.0",
  "description": "Generate an embed code for a Google Maps location",
  "permissions": ["activeTab", "declarativeContent"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["content.js"]
    }
  ]
}



